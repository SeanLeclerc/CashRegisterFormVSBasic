    Public Function ValidatePostalCode(ByVal text_box _
    As TextBox) As Boolean
        Dim txt As String


        ValidatePostalCode = False


        txt = txtCodePostal.Text



        If (UCase$(txt) Like "[A-Z]#[A-Z] #[A-Z]#") Then Exit _
            Function


        MsgBox("Certaines données manquantes. Faites les corrections nécessaires..")



        ValidatePostalCode = True
    End Function

